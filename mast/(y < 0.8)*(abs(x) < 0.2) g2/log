  116 : 
  117 :     border L11(t = 0.4, -0.4){x = t; y = 1.2; label = FREE;}
  118 :     border L21(t = 1.2, 0.8){x = -0.4; y = t; label = FREE;}
  119 :     border L31(t = -0.4, -0.35){x = t; y = 0.8; label = LOAD;}
